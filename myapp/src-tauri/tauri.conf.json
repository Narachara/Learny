{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "learny",
  "version": "0.1.0",
  "identifier": "learny",
  "build": {
    "beforeDevCommand": "dx build --platform web --release",
    "devUrl": null,
    "frontendDist": "../target/dx/learny-ui/release/web/public"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "learny",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["*"]
      },
      "capabilities": ["default"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}