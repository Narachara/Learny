{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "myapp",
  "version": "0.1.0",
  "identifier": "com.myapp.app",
  "build": {
    "beforeDevCommand": "dx build --platform web --release",
    "devUrl": null,
    "frontendDist": "../target/dx/myapp-ui/release/web/public"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "myapp",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
  }
}